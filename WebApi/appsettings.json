{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning", // Set this to Information level
        "System": "Warning" // Set this to Information level
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "FitToFitApp",
      "Workspace": "FitToFit"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Database": "Server=.;Database=FitToFit;integrated security=true;TrustServerCertificate=true;MultipleActiveResultSets=true"
  },
  "AuthSettings": {
    "Issuer": "FitToFit_Identity",
    "Audience": "FitToFit_Services",
    "Secret": "FitToFit_Identity_!@#"
  },
}
